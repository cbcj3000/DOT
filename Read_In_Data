from openpyxl import load_workbook
book = load_workbook('MockData.xlsx')
sheet = book.active

rows = sheet.rows
headers = [cell.value for cell in next(rows)]
all_rows =[]

for row in rows:
    data = {}
    for title, cell in zip(headers, row):
        data[title] = cell.value
    print(data, ' ', sep='\n')
    all_rows.append(data)
